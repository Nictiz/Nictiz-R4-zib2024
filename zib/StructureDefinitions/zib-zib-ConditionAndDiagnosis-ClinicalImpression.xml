    </element>
    <element id="ClinicalImpression.effective[x]">
      <path value="ClinicalImpression.effective[x]" />
      <short value="DiagnosticInsightDate" />
      <definition value="Date (and time) at which the care professional obtained the diagnostic insight." />
      <alias value="DiagnostischInzichtDatum" />
      <type>
        <code value="dateTime" />
      </type>
      <mapping>
        <identity value="zib-diagnosis-v1.0-2024EN" />
        <map value="NL-CM:5.6.3" />
        <comment value="DiagnosticInsightDate" />
      </mapping>
    </element>
    <element id="ClinicalImpression.assessor">
      <path value="ClinicalImpression.assessor" />
      <short value="Diagnostician" />
      <definition value="The care professional that acquired the diagnostic insight. This can be a different individual than the person who recorded the diagnostic insight." />
      <alias value="DiagnoseSteller" />
      <type>
        <code value="Reference" />
        <profile value="http://nictiz.nl/fhir/StructureDefinition/pattern-ZibHealthProfessionalReference" />
        <targetProfile value="http://nictiz.nl/fhir/StructureDefinition/zib-HealthProfessional-PractitionerRole" />
        <targetProfile value="http://hl7.org/fhir/StructureDefinition/Practitioner" />
        <targetProfile value="http://hl7.org/fhir/StructureDefinition/PractitionerRole" />
      </type>
      <mapping>
        <identity value="zib-diagnosis-v1.0-2024EN" />
        <map value="NL-CM:5.6.2" />
        <comment value="Diagnostician" />
      </mapping>
    </element>
    <element id="ClinicalImpression.problem">
      <path value="ClinicalImpression.problem" />
      <short value="Condition" />
      <definition value="The Condition to which the DiagnosticInsight applies." />
      <alias value="AandoeningOfGesteldheid" />
      <max value="1" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://nictiz.nl/fhir/StructureDefinition/zib-ConditionAndDiagnosis" />
        <targetProfile value="http://hl7.org/fhir/StructureDefinition/Condition" />
      </type>
      <mapping>
        <identity value="zib-diagnosis-v1.0-2024EN" />
        <map value="NL-CM:5.6.10" />
        <comment value="Condition" />
      </mapping>
    </element>
    <element id="ClinicalImpression.investigation">
      <path value="ClinicalImpression.investigation" />
      <max value="1" />
    </element>
    <element id="ClinicalImpression.investigation.code">
      <path value="ClinicalImpression.investigation.code" />
      <short value="MethodOfConfirmation" />
      <definition value="The method that the care professional used to diagnose the condition, such as history taking only, history taking and physical examination, additional diagnostic examination or information from other care professionals." />
      <alias value="WijzeVanVaststellen" />
      <binding>
        <strength value="extensible" />
        <valueSet value="http://decor.nictiz.nl/fhir/ValueSet/2.16.840.1.113883.2.4.3.11.60.40.2.5.6.2--20240415000000" />
      </binding>
      <mapping>
        <identity value="zib-diagnosis-v1.0-2024EN" />
        <map value="NL-CM:5.6.5" />
        <comment value="MethodOfConfirmation" />
      </mapping>
    </element>
    <element id="ClinicalImpression.finding.itemCodeableConcept">
      <path value="ClinicalImpression.finding.itemCodeableConcept" />
      <short value="DiagnosisName" />
      <definition value="The term with associated code that the care professional selects from the used code system with conditions." />
      <alias value="DiagnoseNaam" />
      <binding>
        <strength value="required" />
        <valueSet value="http://decor.nictiz.nl/fhir/ValueSet/2.16.840.1.113883.2.4.3.11.60.40.2.5.6.4--20240415000000" />
      </binding>
      <mapping>
        <identity value="zib-diagnosis-v1.0-2024EN" />
        <map value="NL-CM:5.6.7" />
        <comment value="DiagnosisName" />
      </mapping>
    </element>
    <element id="ClinicalImpression.finding.itemCodeableConcept.text">
      <path value="ClinicalImpression.finding.itemCodeableConcept.text" />
      <short value="DiagnosisName" />
      <definition value="The term with associated code that the care professional selects from the used code system with conditions." />
      <alias value="DiagnoseNaam" />
      <mapping>
        <identity value="zib-diagnosis-v1.0-2024EN" />
        <map value="NL-CM:5.6.7" />
        <comment value="DiagnosisName" />
      </mapping>
    </element>
    <element id="ClinicalImpression.prognosisCodeableConcept">
      <path value="ClinicalImpression.prognosisCodeableConcept" />
      <short value="DegreeOfCertainty" />
      <definition value="Indicates the conviction of the care professional with respect to the diagnostic insight as interpretation of the condition." />
      <alias value="ZekerheidStatus" />
      <max value="1" />
      <binding>
        <strength value="required" />
        <valueSet value="http://decor.nictiz.nl/fhir/ValueSet/2.16.840.1.113883.2.4.3.11.60.40.2.5.6.3--20240415000000" />
      </binding>
      <mapping>
        <identity value="zib-diagnosis-v1.0-2024EN" />
        <map value="NL-CM:5.6.4" />
        <comment value="DegreeOfCertainty" />
      </mapping>
    </element>
    <element id="ClinicalImpression.note">
      <path value="ClinicalImpression.note" />
      <max value="1" />
    </element>
    <element id="ClinicalImpression.note.text">
      <path value="ClinicalImpression.note.text" />
      <short value="Comment" />
