    <identity value="zib-diagnosis-v1.0-2024EN" />
    <uri value="https://zibs.nl/wiki/Diagnosis-v1.0(2024EN)" />
    <name value="zib Diagnosis-v1.0(2024EN)" />
  </mapping>
  <kind value="resource" />
  <abstract value="true" />
  <type value="ClinicalImpression" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/ClinicalImpression" />
  <derivation value="constraint" />
  <differential>
    <element id="ClinicalImpression">
      <path value="ClinicalImpression" />
      <short value="DiagnosticInsight" />
      <alias value="DiagnostischInzicht" />
      <mapping>
        <identity value="zib-diagnosis-v1.0-2024EN" />
        <map value="NL-CM:5.6.1" />
        <comment value="DiagnosticInsight" />
